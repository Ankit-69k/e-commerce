{
  "version": 2,
  "builds": [
    {
      "src": "src/server/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**", "/src"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/server.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "PRISMA_CLIENT_ENGINE_TYPE": "dataproxy"
  }
}
